    "*.declaration": { "fontStyle": "bold", "foreground": "#6b40c3" },
    "*.defaultLibrary": { "fontStyle": "", "foreground": "#1d9700" },
    "*.deprecated": {
      "fontStyle": "bold",
      "foreground": "#bf0000"
