  "semanticHighlighting": true,
  "semanticTokenColors": {
    "*.abstract": "#7f51d6",
    "*.async": "#c49700",
    "*.declaration": "#7f51d6",
    "*.defaultLibrary": "#1d9700",
    "*.deprecated": "#d6000c",
    "*.documentation": "#1d9700",
    "*.modification": "#dd0f9d",
    "*.readonly": "#dd0f9d",
    "*.static": "#dd0f9d",
    "class": "#c7008b",
    "comment": "#878787",
    "decorator": "#c7008b",
    "enum": "#c7008b",
    "enumMember": "#dd0f9d",
    "event": "#7f51d6",
    "function": "#c7008b",
    "interface": "#c7008b",
    "keyword": "#6b40c3",
    "label": "#7f51d6",
    "macro": "#dd0f9d",
    "method": "#c7008b",
    "namespace": "#c7008b",
    "number": "#00ad9c",
    "operator": "#6b40c3",
    "parameter": "#dd0f9d",
    "property": "#dd0f9d",
    "regexp": "#0064e4",
    "string": "#00ad9c",
    "struct": "#c7008b",
    "type": "#7f51d6",
    "typeParameter": "#7f51d6",
    "variable": "#dd0f9d"
  },
  "tokenColors": [
    {
      "name": "comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#878787"
      }
    },
    {
      "name": "constants",
      "scope": [
        "constant",
        "constant.character",
        "constant.character.escape",
        "constant.numeric",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.integer",
        "constant.numeric.octal",
        "constant.other",
        "constant.rgb-value"
      ],
      "settings": {
        "foreground": "#00ad9c"
      }
    },
    {
      "name": "emphasis",
      "scope": ["emphasis"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "entitys",
      "scope": [
        "entity",
        "entity.name.selector",
        "entity.other",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#dd0f9d"
      }
    },
    {
      "name": "invalids",
      "scope": ["invalid", "invalid.deprecated", "invalid.illegal"],
      "settings": {
        "foreground": "#bf0000"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "entity.name.tag",
        "keyword",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.switch",
        "keyword.control.trycatch",
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.logical",
        "keyword.operator.new",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#6b40c3"
      }
    },
    {
      "scope": ["markup", "markup.raw", "markup.other"],
      "settings": {
        "foreground": "#474747"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#282828"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#c49700"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#d6000c"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#282828"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#1d9700"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#dd0f9d"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#474747"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#009a8a"
      }
    },
    {
      "name": "metas",
      "scope": [
        "meta",
        "meta.block",
        "meta.cast",
        "meta.class",
        "meta.function",
        "meta.function-call",
        "meta.preprocessor",
        "meta.return-type",
        "meta.selector",
        "meta.tag",
        "meta.type",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#474747"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.separator.continuation",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#474747"
      }
    },
    {
      "name": "storages",
      "scope": ["storage", "storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#7f51d6"
      }
    },
    {
      "name": "strings",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "string",
        "string.interpolated",
        "string.other",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.other",
        "string.quoted.single",
        "string.quoted.triple",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#00ad9c"
      }
    },
    {
      "name": "strongs",
      "scope": ["strong"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "supports",
      "scope": [
        "constant.language.boolean",
        "constant.regexp",
        "string.regexp",
        "support",
        "support.constant",
        "support.other"
      ],
      "settings": {
        "foreground": "#0064e4"
      }
    },
    {
      "name": "variables",
      "scope": [
        "variable",
        "variable.name",
        "variable.other",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#dd0f9d"
      }
    },
    {
      "name": "classes",
      "scope": ["entity.name.class", "entity.other.inherited-class"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#c7008b"
      }
    },
    {
      "name": "functions",
      "scope": ["entity.name.function"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c7008b"
      }
    },
    {
      "name": "methods",
      "scope": ["entity.name.method"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c7008b"
      }
    },
    {
      "name": "sections",
      "scope": ["entity.name", "entity.name.section"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c7008b"
      }
    },
    {
      "name": "supports.classes",
      "scope": ["support.class"],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#008400"
      }
    },
    {
      "name": "supports.functions",
      "scope": ["support.function"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#008400"
      }
    },
    {
      "name": "supports.variables",
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#1d9700"
      }
    },
    {
      "name": "types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#7f51d6"
      }
    },
    {
      "name": "variables.languajes",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#1d9700"
      }
    }
  ]
}
