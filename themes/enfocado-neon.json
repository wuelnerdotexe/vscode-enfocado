  "semanticHighlighting": true,
  "semanticTokenColors": {
    "*.abstract": "#a580e2",
    "*.async": "#dbb32d",
    "*.declaration": "#a580e2",
    "*.deprecated": "#ed4a46",
    "*.documentation": "#eb6eb7",
    "*.modification": "#eb6eb7",
    "*.readonly": "#eb6eb7",
    "*.static": "#eb6eb7",
    "class": "#b891f5",
    "comment": "#777777",
    "enum": "#b891f5",
    "enumMember": "#3fc5b7",
    "event": "#3fc5b7",
    "function": "#eb6eb7",
    "interface": "#b891f5",
    "keyword": "#a580e2",
    "label": "#a580e2",
    "macro": "#a580e2",
    "method": "#eb6eb7",
    "namespace": "#b891f5",
    "number": "#eb6eb7",
    "operator": "#a580e2",
    "parameter": "#eb6eb7",
    "property": "#eb6eb7",
    "regexp": "#eb6eb7",
    "string": "#3fc5b7",
    "struct": "#b891f5",
    "type": "#a580e2",
    "typeParameter": "#a580e2",
    "variable": "#eb6eb7"
  },
  "tokenColors": [
    {
      "name": "comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#777777"
      }
    },
    {
      "name": "constants",
      "scope": [
        "constant",
        "constant.character",
        "constant.character.escape",
        "constant.numeric",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.integer",
        "constant.numeric.octal",
        "constant.other",
        "constant.regexp",
        "constant.rgb-value",
        "support.constant"
      ],
      "settings": {
        "foreground": "#3FC5B7"
      }
    },
    {
      "name": "emphasis",
      "scope": ["emphasis"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EFC541"
      }
    },
    {
      "name": "entitys",
      "scope": [
        "entity",
        "entity.name",
        "entity.name.selector",
        "entity.other",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#EB6EB7"
      }
    },
    {
      "name": "invalids",
      "scope": ["invalid", "invalid.deprecated", "invalid.illegal"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF5E56"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.switch",
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.logical",
        "keyword.operator.new",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#A580E2"
      }
    },
    {
      "scope": ["markup", "markup.raw", "markup.other"],
      "settings": {
        "foreground": "#DEDEDE"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#DEDEDE"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EFC541"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF5E56"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4F9CFE"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#83C746"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DEDEDE"
      }
    },
    {
      "scope": [
        "markup.list",
        "markup.list.numbered",
        "markup.list.unnumbered"
      ],
      "settings": {
        "foreground": "#EB6EB7"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B9B9B9"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#DEDEDE"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#B891F5"
      }
    },
    {
      "name": "metas",
      "scope": [
        "meta",
        "meta.block",
        "meta.cast",
        "meta.class",
        "meta.function",
        "meta.function-call",
        "meta.preprocessor",
        "meta.return-type",
        "meta.selector",
        "meta.type",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#DEDEDE"
      }
    },
    {
      "name": "punctuation",
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#DEDEDE"
      }
    },
    {
      "name": "storages",
      "scope": ["storage", "storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#A580E2"
      }
    },
    {
      "name": "strings",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "string",
        "string.interpolated",
        "string.other",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.other",
        "string.quoted.single",
        "string.quoted.triple",
        "string.regexp",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#3FC5B7"
      }
    },
    {
      "name": "strongs",
      "scope": ["strong"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#EFC541"
      }
    },
    {
      "name": "supports",
      "scope": [
        "support",
        "support.function",
        "support.other",
        "support.variable"
      ],
      "settings": {
        "foreground": "#368AEB"
      }
    },
    {
      "name": "variables",
      "scope": [
        "variable",
        "variable.name",
        "variable.other",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#EB6EB7"
      }
    },
    {
      "name": "classes",
      "scope": [
        "entity.name.class",
        "entity.name.section",
        "entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#B891F5"
      }
    },
    {
      "name": "exceptions",
      "scope": ["keyword.control.trycatch"],
      "settings": {
        "foreground": "#E67F43"
      }
    },
    {
      "name": "functions",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#EB6EB7"
      }
    },
    {
      "name": "methods",
      "scope": ["entity.name.method"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#EB6EB7"
      }
    },
    {
      "name": "support.class",
      "scope": ["support.class"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4F9CFE"
      }
    },
    {
      "name": "tags",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "punctuation.separator.key-value.html"
      ],
      "settings": {
        "foreground": "#A580E2"
      }
    },
    {
      "name": "types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#A580E2"
      }
    },
    {
      "name": "variable.languajes",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#368AEB"
      }
    }
  ]
}
